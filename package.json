{
	"name": "teoscommands",
	"displayName": "teoscommands",
	"publisher": "teotoivo",
	"icon": "icon.png",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/teotoivo/teoscommands"},
	"description": "adds some new commands",
	"version": "1.0.4",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished",
		"onCommand:teoscommands.buildTsProject",
		"onCommand:teoscommands.buildReactProject"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "teoscommands.buildTsProject",
				"title": "build new ts project"
			},
			{
				"command": "teoscommands.buildReactProject",
				"title": "build new react project"
			}
		],
		"configuration": {
			"title": "teoscommands",
			"properties": {
			  	"teoscommands.folders": {
					"type": "array",
					"default": [
						"src/public/js/script.ts",
						"src/index.ts",
						"dev/public/js",
						"dev/public/css",
						"dev/public/html"
					],
					"description": "spesifies what folders/files should be created on build"
			  	}
		  	}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^8.0.0",
		"@types/mocha": "^10.0.1",
		"@types/node": "^16.18.11",
		"@types/vscode": "^1.74.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"@vscode/test-electron": "^2.2.0",
		"eslint": "^8.28.0",
		"glob": "^8.0.3",
		"mocha": "^10.1.0",
		"typescript": "^4.9.3"
	}
}